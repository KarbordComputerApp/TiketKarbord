@{
    ViewBag.Title = " ";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <div class="form-inline">
                    <strong id="TitleHeaderAnbar"></strong>
                    <strong style="margin-left: 5px; margin-right: 5px;" data-bind="text:translate('به شماره')"></strong>

                    <div class="form-inline">
                        <div class="form-group form-float" style="margin-bottom: 0px;">
                            <div class="form-line focused">
                                <input data-bind=" event:{ keydown : $root.docNoKeyDown}" id="docnoout" class="form-control" style="width: 70px;text-align: center;" maxlength="10" />
                            </div>
                        </div>
                    </div>

                    <div class="input-group" style="margin-right: 10px;">
                        <div class="input-group-addon">
                            <span class="input-group-btn">
                                <a id="btntarikh"><img src="~/Content/img/list/calendar.png" data-bind="attr: {title:text_Date}" width="16" height="16" /></a>
                            </span>
                        </div>
                        <div class="form-group form-float">
                            <div class="form-line">
                                <input id="tarikh" data-bind="value:DocDate, attr:{placeholder:translate('تاریخ')}" class="form-control date focused fix" type="text" maxlength="10" style="text-indent: 5px;width: 100px;text-align: center;">
                            </div>
                        </div>
                    </div>

                    <lable id="finalSave_Title" style="margin: auto; width: 50%; color: coral;" data-bind="text:translate('(( برای ثبت تغییرات کلید ثبت نهایی را بزنید ))')"></lable>

                    <div id="buttom-header-dropdown" class="row header-dropdown m-r--5">

                        <a id="Print_SanadAnbar" style="margin-left: 10px;">
                            <img src="/Content/img/sanad/streamline-icon-print-text@48x48.png" width="20" height="20">
                        </a>

                        <!--                        <a id="SettingColumn" class="">
                            <img src="~/Content/img/sanad/list-add.png" width="20" height="20" style="margin-left: 10px;" class="dropdown-toggle" data-toggle="modal" data-target="#modal-SettingColumn" />
                        </a>-->
                        <a id="OtherField" data-bind="attr: {title:text_OtherField}" class="dropdown-toggle" data-toggle="modal" data-backdrop="static" data-keyboard="false" data-target="#modal-OtherField" style="margin-left: 10px;">
                            <img src="~/Content/img/sanad/paper-write.png" width="20" height="20" />
                        </a>
                        <a id="AddNewSanadAnbar">
