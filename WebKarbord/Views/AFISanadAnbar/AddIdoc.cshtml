                                   value: InvCode,
                                   optionsCaption: OptionsCaptionAnbar()
                                                "></select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div style="margin-left: 5px; margin-right: 5px;">
                                    <div class="form-group" style="margin-bottom: 10px;">
                                        <select id="modeCode" data-bind="options: IModeList,
                                   optionsText: function(item) {return item.Name},
                                   optionsValue: 'Code',
                                   value: modeCode"></select>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-3" id="ViewGGhimat" style="margin-bottom: 10px;">
                            <div class="form-inline" style="margin-left: 5px; margin-right: 5px;">
                                <select id="gGhimat" data-bind="options: KalaPriceList,
