                        <div id="action_Hdoc">
                            <button data-bind="text:translate('ثبت نهایی')" type="button" id="FinalSave" class="btn btn-primary btn-block" style="background-color: #eb8121 !important;"></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

