                                    <th data-bind="text:translate('مقدار 3')">

                                    </th>
                                    <th data-bind="text:translate('قیمت واحد')" id="unitpriceshowgridtitle">

                                    </th>
                                    <th data-bind="text:translate('قیمت کل')" id="totalpriceshowgridtitle">

                                    </th>

                                    <th style="width: 160px" data-bind="text:translate('شرح')">

                                    </th>
                                    <th id="action_header" data-bind="text:translate('عملیات')"></th>
                                </tr>
                            </thead>

                            <tbody data-bind="foreach: IDocBList" style="cursor: default;">
                                <tr data-bind="click: $parent.selectFactor">
                                    <td data-bind="text: BandNo" id="textBandNo"></td>
                                    <td data-bind="text: KalaCode"></td>
                                    <td data-bind="text: KalaName"></td>
                                    <td data-bind="text: MainUnitName"></td>
                                    <td data-bind="text: NumberToNumberString(Amount1.toFixed(KalaDeghatM1))"></td>
                                    <td data-bind="text: Amount2 == 0 ? '' : NumberToNumberString(Amount2.toFixed(KalaDeghatM2)) "></td>
                                    <td data-bind="text: Amount3 == 0 ? '' : NumberToNumberString(Amount3.toFixed(KalaDeghatM3)) "></td>
                                    <td data-bind="text: NumberToNumberString(UnitPrice.toFixed(DeghatR))" id="unitpriceshowgridbody"></td>
                                    <td data-bind="text: NumberToNumberString(NumberToNumberString(parseFloat(TotalPrice).toFixed(parseInt(sessionStorage.Deghat))))" id="totalpriceshowgridbody"></td>
                                    <td data-bind="text: Comm" class="ellipsis"></td>
                                    <td id="action_body">
                                        <a data-bind="click: $root.UpdateBand">
                                            <img src="~/Content/img/list/streamline-icon-pencil-write-2-alternate@48x48.png" width="16" height="16" style="margin-left:10px" />
                                        </a>
                                        <a data-bind="click: $root.DeleteBand , attr: {title:text_Delete}">
                                            <img src="~/Content/img/list/streamline-icon-bin-2@48x48.png" width="16" height="16" style="margin-left:10px" />
                                        </a>
                                        <a data-bind="click: $root.ImportBand" data-toggle="modal" data-backdrop="static" data-keyboard="false" data-target="#modal-Band">
                                            <img src="~/Content/img/sanad/streamline-icon-logout-alternate@48x48.png" width="16" height="16" />
                                        </a>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr id="footertext" style="background-color:#e37d228f;">
                                    <td id="foottextsum"> </td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td id="foottextamount1"> </td>
                                    <td id="foottextamount2"> </td>
                                    <td id="foottextamount3"> </td>
                                    <td id="foottextunitprice"></td>
                                    <td id="foottexttotalprice"></td>
                                    <td></td>
                                    <td id="action_footer"></td>
                                </tr>


                                <!-- <tr id="footertext" style="background-color:#e37d228f;">
                                    <td style="background-color: #e37d228f !important;" id="foottextsum"> </td>
                                    <td style="background-color: #e37d228f !important;"></td>
                                    <td style="background-color: #e37d228f !important;"></td>
