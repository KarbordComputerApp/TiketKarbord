@{
    ViewBag.Title = "کاربرد کامپیوتر";
    Layout = "~/Views/Shared/_LayoutLogin.cshtml";
}


<div class="row">
    <div class="col-lg-6 PanelLeftLogin d-none d-lg-block">
        <div class="row" style="text-align:center;">
            <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
            </div>

            <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
                <img id="LogoLogin" src="~/Content/img/Login/LogoLogin.png" style="padding: 35% 20% 0% 20%; max-width:100%" />
                <div style="padding: 35% 20% 0% 20%;">
                    <p style="color: #fefefe;" data-bind="text:translate('کلیه حقوق این نرم افزار متعلق به شرکت مهندسی کاربرد کامپیوتر بوده و هر گونه نسخه برداری پیگرد قانونی دارد')" >
                        
                    </p>
                </div>
            </div>
        </div>
    </div>

    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 " id="datalogin">

        <div class="row">
            <div class="col-lg-4 col-md-3 col-sm-2 col-xs-2">
            </div>

            <div class="col-lg-4 col-md-6 col-sm-8 col-xs-8">

                <div class="login-form" style="direction:rtl; padding-top: 100%">
                    <!--padding: 40% 30% 0% 30%;-->
                    <div class="col-sm-12">
                        <div class="form-group form-float ">
                            <div class="form-line focused fix">
                                <input type="text" id="user" class="form-control" data-bind="attr:{placeholder:translate('نام کاربری')}" style="border-bottom: 1px solid #ec8121;text-indent: 20px;direction: rtl; color: #f3f4f3;" />
                                <img class="form-label" src="~/Content/img/Login/streamline-icon-single-man@48x48.png" width="16" height="16" />
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-12">
                        <div class="form-group form-float">
                            <div class="form-line focused fix">
                                <input type="password" id="pass" class="form-control" data-bind="attr:{placeholder:translate('کلمه عبور')}" style="border-bottom: 1px solid #ec8121;text-indent: 20px;direction: rtl; color: #f3f4f3;"  />
                                <img class="form-label" src="~/Content/img/Login/streamline-icon-lock-5@48x48.png" width="16" height="16" />
                            </div>
                        </div>
                    </div>

                    <br />
                    <br />

                    <div class="form-group btn-container">
                        <button type="button" class="btn btn-primary btn-block" data-bind="click:LoginUser , text:translate('ورود')" id="Login" style="background-color: #eb8121 !important;"></button>
                    </div>

                    <div class="form-group" style="padding-top:100px;text-align: center;">

                        <a class="col-md-12" data-toggle="modal" data-backdrop="static" data-keyboard="false" data-target="#modal-service" style="color: #fdfdfd;">

                            <img src="~/Content/img/Login/streamline-icon-add-circle-bold@48x48.png" width="16" height="16" />
                           <span data-bind="text:translate('تنظیمات اتصال به وب')"></span> 
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="modalTwo modal modal-info fade" id="modal-service">
    <div class="modal-dialog" style="width: 400px;">
        <div class="modal-content" style="background-color: #ededef;">
            <div class="modal-header">

            </div>
            <div class="modal-body">

                <div>
                    <h5 class="" style="text-align: center; padding: 25px;" data-bind="text:translate('تنظیمات اتصال به وب')"></h5>
                </div>
                <br />
                <br />
                <div class="col-sm-12">
                    <div class="form-group form-float" style="  border: 1px solid white;
                                                                border-radius: 10px;
                                                                padding: 20px;
                                                                background-color: white;
                                                                box-shadow: 5px 5px 20px 1px #4048524f;">
                        <div class="form-line">
                            <input type="text" id="userAccount" class="form-control login" data-bind="attr:{placeholder:translate('نام مجوز ورود')}" style="border-bottom: 1px solid #ec8121;text-indent: 20px;direction: rtl;" />
                            <img class="form-label" src="~/Content/img/Login/streamline-icon-single-man@48x48.png" width="16" height="16" />
                        </div>
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="form-group form-float" style="  border: 1px solid white;
                                                                border-radius: 10px;
                                                                padding: 20px;
                                                                background-color: white;
                                                                box-shadow: 5px 5px 20px 1px #4048524f;">
                        <div class="form-line">
                            <input type="password" id="passAccount" data-bind="attr:{placeholder:translate('کلمه عبور')}" class="form-control login" style="border-bottom: 1px solid #ec8121;text-indent: 20px;direction: rtl;" placeholder="" />
                            <img class="form-label" src="~/Content/img/Login/streamline-icon-lock-5@48x48.png" width="16" height="16" />
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal-footer">
                <div class="form-group btn-container" style="padding: 20px;">
                    <button type="button" class="btn btn-primary btn-block" data-bind="click:LoginAccount,text:translate('اتصال')" id="ConnectAccount" style="background-color: #eb8121 !important; margin-left:0px;"></button>
                </div>
            </div>
        </div>
    </div>

</div>




<div class="modalTwo modal modal-info fade" id="modal-dataUser">
    <div class="modal-dialog" style="width: 400px; direction: rtl;">
        <div class="modal-content" style="background-color: #ededef;">
            <div class="modal-header">

            </div>
            <div class="modal-body">

                <div>
                    <h5 id="title_dataUser" style="text-align: center; "></h5>
                </div>
                <br />
                <br />

                <div class="table-responsive">
                    <table class="table" style="text-align: start;">
                        <tbody>
                            <tr>
                                <th style="width:50%"data-bind="text:translate('ای پی')"></th>
                                <td id="param_ipw" style="direction:ltr"></td>
                            </tr>
                            <tr>
                                <th data-bind="text:translate('تاریخ ورود')" ></th>
                                <td id="param_date" style="direction:ltr"></td>
                            </tr>
                            <tr>
                                <th data-bind="text:translate('ساعت ورود')"></th>
                                <td id="param_time" style="direction:ltr"></td>
                            </tr>
                            <tr>
                                <th data-bind="text:translate('نام نرم افزار')"></th>
                                <td id="param_prog"></td>
                            </tr>
                            <tr>
                                <th data-bind="text:translate('نسخه نرم افزار')"></th>
                                <td id="param_ver" style="direction:ltr"></td>
                            </tr>
                            <!-- <tr>
                                 <th>کشور</th>
                                 <td id="param_country"></td>
                             </tr>
                             <tr>
                                 <th>شهر</th>
                                 <td id="param_city"></td>
                             </tr>-->
                        </tbody>
                    </table>
                </div>

            </div>

            <div class="modal-footer">
                
                <div class="row col-md">

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6" style="padding-right: 8px;">
                        <button id="Login_LogOutUser" data-bind="text:translate('ورود')" class="btn btn-primary btn-block " style="background-color: #eb8121 !important; padding-left: 0px; padding-right: 0px"></button>
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <button data-bind="text:translate('انصراف')" type="button" data-dismiss="modal" class="btn btn-primary btn-block" style="background-color: #eb8121 !important;  padding-left: 0px; padding-right: 0px"></button>
                    </div>
                </div>

               <!-- <div class="row">
                    
                        <button data-bind="text:translate('بازگشت')" type="button" data-dismiss="modal" class="btn btn-primary btn-block" style="background-color: #eb8121 !important; margin-left:0px;"></button>
                        <button data-bind="text:translate('ورود')" class="btn btn-primary btn-block" style="background-color: #eb8121 !important; margin-left:0px;"></button>
                    
                </div>-->
            </div>
        </div>
    </div>

</div>


