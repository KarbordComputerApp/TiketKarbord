                                <td style="direction: ltr;" data-bind="text: TotalPrice != null ? NumberToNumberString(parseFloat(TotalPrice).toFixed(parseInt(sessionStorage.Deghat))) : '0' , style: { color: TotalPrice < 0 ? 'red' : 'black' }" style="text-align: right;"></td>

                                <td data-bind="text: BandSpec"></td>
                                <td data-bind="text: Comm"></td>

                            </tr>
